<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.groundzeroapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".fieldofviewwidening.Pyramids"
            android:label="@string/title_activity_field_of_view_grid_one"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".fieldofviewwidening.SchultzTables"
            android:label="@string/title_activity_schultz_tables"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".fieldofviewwidening.WideningAnimation"
            android:label="@string/title_activity_widening_animation"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".fieldofviewwidening.WideningText"
            android:label="@string/title_activity_text_pyramids"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>